{
  "Urls": "http://localhost:5000;https://localhost:5001",
  "ConnectionStrings": {
    "Telemetry": "http://localhost:4317"
  },
  "ReverseProxy": {
    "Routes": {
      "events": {
        "RouteId": "events",
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/events/{*any}"
        }
      },
      "users": {
        "RouteId": "users",
        "ClusterId": "cluster2",
        "Match": {
          "Path": "/users/{*any}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "ClusterId": "cluster1",
        "Destinations": {
          "first_destination": {
            "Address": "https://localhost:7169"
          }
        },
        "LoadBalancingPolicy": "PowerOfTwoChoices"
      },
      "cluster2": {
        "ClusterId": "cluster2",
        "Destinations": {
          "first_destination": {
            "Address": "https://localhost:7170"
          }
        },
        "LoadBalancingPolicy": "PowerOfTwoChoices"
      }
    }
  }
}